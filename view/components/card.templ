package components

import "github.com/Melidee/goth-chat/model"

templ Card(user model.User) {
    <div class="bg-sky-300">
        <h5>card!</h5>
        <img src={user.ProfilePicture}/>
        <div>
            <h3>{user.Name}</h3>
            <h5>{user.Email}</h5>
            <h5>{user.PasswordHash}</h5>
        </div>
    </div>
}